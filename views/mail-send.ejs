      <div class="form-group">
        <label for="role">Role</label>
        <select name="role" required>
          <option value="Mukhiya">Mukhiya</option>
          <option value="Ward">Ward</option>
        </select>
      </div>

      <div class="form-group">
        <label for="from">Your Email</label>
        <input type="email" name="from" value="codewithshivam24@gmail.com"  required>
      </div>

      <div class="form-group">
        <label for="to">Recipient Email</label>
        <select name="to" required>
          <option value="shivam857n@gmail.com">shivam857n@gmail.com</option>
        </select>
      </div>

      <div class="form-group">
        <label for="subject">Subject</label>
        <input type="text" name="subject" value="Problem Accepted Message Notice" readonly required>
